{
  "name": "El Pais - Premium Account",
  "description": "Premium articles get unblocked",
  "version": "1.0",
  "minimum_chrome_version": "10.0",
  "manifest_version": 3,
  "content_scripts":[
    {
      "matches": ["https://*.elpais.com/*"],
      "js": ["contentScript.js"]
    }
  ],

  "action": {
    "default_title": "El Pais Premium",
    "default_icon": "read.png"
  },

  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": [
    "*://*/*"
  ]
}
